

void add_vias(cell* curCell) {

    double n_a = 7*2;
        
    for (int i = -n_a; i<n_a+1; i++) {
        for (int j = -n_a; j<n_a+1; j++) {
            
            point center = newPoint(i * r_PCB/n_a , j * r_PCB/n_a );
            curCell->addCircle(aux2, center, r_via);
            
        }
        
    }
    

    layout->booleanTool->boolOnLayer(aux2, protect, aux3, "A-B", 0, 0, 0);
    layout->drcTool->inside(200e3, aux2, top);

    layout->booleanTool->boolOnLayer(aux2, error, via, "A-B", 0, 0, 0);
    string curCellName = curCell->cellName;
    layout->drawing->setCell(curCellName+":DRC-Rule");
    layout->drawing->deleteCurrentCell();
    layout->drawing->setCell(curCellName);
    clearAllAuxiliaryAux(curCell);

}
